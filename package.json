{
  "name": "home-inventory",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "apps/**"
  ],
  "scripts": {
    "format:quick": "pretty-quick --staged --pattern \"packages/**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"packages/**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "yarn eslint . --ext .ts,.js,.tsx,.jsx",
    "dev": "yarn turbo run watch --parallel",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.4.0",
    "@casper124578/eslint-config-next": "^4.4.0",
    "@casper124578/eslint-config-react": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "eslint": "^8.21.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "turbo": "^1.4.0",
    "typescript": "^4.7.4"
  }
}